<h1>{{title}}</h1>
<main>
  <a href="/tasks/create">
    Click here to create a task!
  </a>
  <br />
  <a href="/tasks/all">
    Click here to view all tasks!
  </a>
  <br />
  <a href="/tasks/private">
    Click here to view all private tasks!
  </a>
  <br />
  <a href="/tasks/public">
    Click here to view all public tasks that you have accepted!
  </a>
  <br />
  <a href="/tasks/forum">
    Click here to view the forum of available public tasks!
  </a>
  <br />
  <a href="/users">
    Click here to return to users!
  </a>
  <br />
  <a href="/logout">
    Click here to logout!
  </a>

  <h1>{{Schedule}}</h1>
    <table>
        <thead>
        <tr>
            <th>Date</th>
            <th>Time</th>
            <th>Task</th>
        </tr>
        </thead>
        <tbody>
        {{#each schedules}}
            <tr>
            <td class="date">{{this.date}}</td>
            <td class="time">{{this.time}}</td>
            <td class="task">{{this.task}}</td>
            </tr>
        {{/each}}
        </tbody>
    </table>

    <br />
    <a href="/users">
        Return to Users
    </a>
    <br />
</main>
